#include <iostream>
using namespace std;

int sumofdigits(int n)
{
    int rem,sum=0;
    while(n>0)
    {
        rem=n%10;
        sum=sum+rem;
        n=n/10;
    }
    return sum;
}

int main() {
    int n;
    cin>>n;
    int x=sumofdigits(n);
    cout<<"Sum of digits of a number "<<n<<" is "<<x;
    
    return 0;
  
}
